{
    "name": "liip/theme-bundle",
    "type": "symfony-bundle",
    "description": "Provides theming support for #Symfony2 Bundles",
    "keywords": ["themes", "theming"],
    "license": "MIT",
    "authors": [
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/liip/LiipThemeBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.3.9|^7.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/finder": "~2.3|~3.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "symfony/console": "~2.3|~3.0",
        "symfony/expression-language": "~2.6|~3.0",
        "symfony/templating": "~2.3|~3.0",
        "phpunit/phpunit": "~5.7",
        "kriswallsmith/assetic": "~1.1",
        "twig/twig": "~1.27|~2.0@dev"
    },
    "autoload": {
        "psr-4": { "Liip\\ThemeBundle\\": "" }
    },
    "conflict": {
        "sebastian/comparator": "1.2.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
